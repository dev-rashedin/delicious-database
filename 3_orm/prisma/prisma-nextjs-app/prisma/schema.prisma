generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id          Int      @id @default(autoincrement())
  name        String
  email       String   @unique
  created_at  DateTime @default(now())
  age         Int
  isMarried   Boolean
  nationality String
}
